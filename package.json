{
  "name": "rss-impact-server",
  "version": "0.1.0",
  "description": "一个基于 RSS 的 Hook 系统。本项目为后端部分。",
  "keywords": [
    "RSS",
    "Hook",
    "Push"
  ],
  "author": "CaoMeiYouRen",
  "license": "AGPL-3.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/main.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src test --fix --ext .ts,.js",
    "release": "semantic-release",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "commit": "cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@cao-mei-you-ren/qbittorrent": "^1.0.3",
    "@ctrl/qbittorrent": "^8.0.0",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-history-api-fallback": "^2.0.0",
    "connect-sqlite3": "^0.9.13",
    "compare-func": "2.0.0",
    "cron": "^3.1.6",
    "crypto-hash": "https://github.com/CaoMeiYouRen/crypto-hash.git",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.0",
    "file-stream-rotator": "^1.0.0",
    "file-type": "16.5.4",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "mysql2": "^3.9.4",
    "p-limit": "^3.1.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "passport-strategy": "^1.0.0",
    "push-all-in-one": "^3.4.5",
    "query-string": "^7.1.3",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "rss-parser": "https://github.com/CaoMeiYouRen/rss-parser.git",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.0",
    "torrent2magnet-js": "https://github.com/CaoMeiYouRen/torrent2magnet-js.git",
    "tslib": "^2.6.2",
    "turndown": "^7.1.3",
    "typeorm": "^0.3.20",
    "xregexp": "^5.1.1",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@cao-mei-you-ren/avue-types": "^1.1.0",
    "@commitlint/config-conventional": "^18.6.3",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/connect-history-api-fallback": "^1.5.4",
    "@types/connect-sqlite3": "^0.9.5",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/md5": "^2.3.5",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.12.7",
    "@types/passport-local": "^1.0.38",
    "@types/passport-strategy": "^0.2.38",
    "@types/supertest": "^6.0.2",
    "@types/turndown": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "commitizen": "^4.2.3",
    "conventional-changelog-cmyr-config": "^2.1.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog-cmyr": "^1.1.0",
    "debug": "^4.3.4",
    "eslint": "^8.57.0",
    "eslint-config-cmyr": "^1.1.30",
    "eslint-plugin-import": "^2.28.1",
    "gulp": "^5.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "semantic-release": "21.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "overrides": {
      "@types/superagent": "4.1.14",
      "crypto-hash": "$crypto-hash",
      "compare-func": "$compare-func"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "express",
        "webpack"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog-cmyr"
    }
  },
  "homepage": "https://github.com/CaoMeiYouRen/rss-impact-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/rss-impact-server.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/rss-impact-server/issues"
  },
  "changelog": {
    "language": "zh"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  }
}
