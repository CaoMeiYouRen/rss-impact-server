    "typescript": "^5.4.5"
  },
  "pnpm": {
    "overrides": {
      "@types/methods": "^1.1.4",
      "@types/superagent": "^8.1.0",
      "axios": "$axios",
      "crypto-hash": "$crypto-hash",
      "compare-func": "$compare-func",
      "cron": "$cron",
      "formidable": "^3.2.4",
      "tough-cookie": "^4.1.3"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "express",
        "webpack"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog-cmyr"
    }
  },
  "homepage": "https://github.com/CaoMeiYouRen/rss-impact-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/rss-impact-server.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/rss-impact-server/issues"
  },
  "changelog": {
    "language": "zh",
    "settings": {
      "test": {
        "enable": true
      }
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "npm run lint:md",
      "git add"
    ]
  }
}