    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "nest-winston": "^1.10.0",
    "openai": "^4.51.0",
    "opml": "^0.5.5",
    "p-limit": "^3.1.0",
    "parse-torrent": "9.1.5",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "passport-strategy": "^1.0.0",
    "push-all-in-one": "^3.5.3",
    "query-string": "^7.1.3",
    "rate-limit-redis": "^4.2.0",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.7",
    "rss-parser": "git+https://github.com/CaoMeiYouRen/rss-parser.git",
    "rxjs": "^7.8.1",
    "socks-proxy-agent": "^8.0.3",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.6.3",
    "turndown": "^7.2.0",
    "typeorm": "^0.3.20",
    "validator": "^13.12.0",
    "winston": "^3.13.0",
